{
  "name": "ava239/validator",
  "bin": ["bin/validator"],
  "type": "library",
  "version": "1.3.2",
  "authors": [
    {
      "name": "Dmitrii Oliferchuk",
      "email": "ava239@yandex.ru"
    }
  ],
  "description": "data validator",
  "license": "MIT",
  "scripts": {
    "test": "phpunit tests"
  },
  "autoload": {
    "psr-4": {
      "Ava239\\Validator\\": "src",
      "Ava239\\Validator\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10.38",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpstan/extension-installer": "^1.3.1",
    "symfony/var-dumper": "^6.3.5",
    "phpunit/phpunit": "^10.4.1",
    "squizlabs/php_codesniffer": "^3.5.5"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
